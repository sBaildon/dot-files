{
    "editor.fontFamily": "Operator Mono, SF Mono, Menlo, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.fontWeight": "500",
    "editor.minimap.enabled": false,
    "editor.renderFinalNewline": false,
    "editor.tabSize": 8,
    "files.autoSave": "onFocusChange",
    "files.autoSaveDelay": 0,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "go.useLanguageServer": true,
    "telemetry.enableTelemetry": false,
    "window.autoDetectColorScheme": true,
    "window.nativeTabs": true,
    "workbench.colorTheme": "Tomorrow Operator Mono",
    "workbench.preferredDarkColorTheme": "Tomorrow Night Operator Mono",
    "workbench.preferredLightColorTheme": "Tomorrow Operator Mono",
    "workbench.tree.indent": 24,
}
