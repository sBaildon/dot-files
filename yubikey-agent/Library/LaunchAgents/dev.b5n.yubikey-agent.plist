<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>dev.b5n.yubikey-agent</string>

		<key>ProgramArguments</key>
		<array>
			<string>/Users/sbaildon/.nix-profile/bin/yubikey-agent</string>
			<string>-l</string>
			<string>/tmp/yubikey-agent.sock</string>
		</array>

		<key>StandardOutPath</key>
		<string>/tmp/log/yubikey-agent/stdout.txt</string>
		<key>StandardErrorPath</key>
		<string>/tmp/log/yubikey-agent/stderr.txt</string>

		<key>KeepAlive</key>
		<true/>
	</dict>
</plist>
